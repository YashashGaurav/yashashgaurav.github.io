{
  "enabledPlugins": {
    "frontend-design@claude-plugins-official": true
  },
  "hooks": {
    "WorktreeCreate": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash -c 'REPO=$(cd \"$(git rev-parse --git-common-dir)/..\" && pwd) && exec \"$REPO/.claude/hooks/setup-worktree.sh\"'",
            "timeout": 120
          }
        ]
      }
    ]
  }
}
